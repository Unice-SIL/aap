{% extends 'admin_layout.html.twig' %}
{% set body_class = 'call-of-project-form control-sidebar-push  control-sidebar-slide-open' %}

{% block title %}
    {{ 'app.admin.project_form_layout.configure.h1'|trans }}
{% endblock %}

{% block h1_content %}
    {{ 'app.admin.project_form_layout.configure.h1'|trans }}
{% endblock %}

{% block modals %}
    {{ parent() }}
    {{ include('partial/modal/_widget_form.html.twig') }}
    {{ include('partial/modal/_alert_form.html.twig') }}
{% endblock %}

{% block content %}
    {{ dynamic_form_html|raw }}
{% endblock %}

{% block control_sidebar %}
    {{ include('partial/widget/_control_sidebar.html.twig', { 'project_form_layout' : project_form_layout}) }}
{% endblock %}