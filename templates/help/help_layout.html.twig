{% extends 'base.html.twig' %}

{% block title %}
    {{ 'app.help.title'|trans }} - {{ title|trans }}
{% endblock %}

{% block modals %}{% endblock %}

{% block body %}
    <div class="container pt-5 help">
        {% block help_title %}
            <h1 class="text-primary">{{ title|trans }}</h1>
        {% endblock %}
        {% block help_content %}{% endblock %}
    </div>
{% endblock %}